namespace test@1.0.0

concept Address {
    o String street
    o String city
    o String zip
}

concept Order {
    o String sku
    o Integer amount
}

concept LoyaltyStatus {
    o String level
}

enum Color {
    o RED
    o GREEN
    o BLUE
    o PINK
    o BLACK
    o WHITE
}

@template
concept TemplateData {
    o String firstName
    o String lastName
    o String[] middleNames
    o Boolean active
    o DateTime lastVisit
    o Address address
    o Order[] orders
    o LoyaltyStatus loyaltyStatus optional
    o Color[] favoriteColors optional
}